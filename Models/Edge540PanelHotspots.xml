<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
  <name>EDM700PanelHotspots</name>
  <background>Aircraft/ZivkoEdge/Models/transparent.rgb</background>
  <w>798</w>
  <h>200</h>
  <instruments>
    <instrument>
      <name>panel hotspots</name>
      <x>399</x>
      <y>100</y>
      <w>798</w>
      <h>200</h>
      <w-base>798</w-base>
      <h-base>200</h-base>

      <actions>

        <!-- TRIM DOWN -->
        <action>
          <name>TrimDown</name>
          <button>0</button>
          <x>-300</x>
          <y>10</y>
          <w>20</w>
          <h>10</h>
          <repeatable type="bool">true</repeatable>
          <binding>
            <command>property-adjust</command>
            <property>controls/flight/elevator-trim</property>
            <step>-0.01</step>
            <min>-1</min>
            <max>1</max>
            <wrap>false</wrap>
          </binding>
          <binding>
            <command>property-assign</command>
            <property>controls/flight/elevator-trim-switch</property>
            <value>1</value>
          </binding>
          <mod-up>
            <binding>
              <command>property-assign</command>
              <property>controls/flight/elevator-trim-switch</property>
              <value>0</value>
            </binding>
          </mod-up>
        </action>

        <!-- TRIM UP -->
        <action>
          <name>TrimUP</name>
          <button>0</button>
          <x>-300</x>
          <y>20</y>
          <w>20</w>
          <h>10</h>
          <repeatable type="bool">true</repeatable>
          <binding>
            <command>property-adjust</command>
            <property>controls/flight/elevator-trim</property>
            <step>0.01</step>
            <min>-1</min>
            <max>1</max>
            <wrap>false</wrap>
          </binding>
          <binding>
            <command>property-assign</command>
            <property>controls/flight/elevator-trim-switch</property>
            <value>-1</value>
          </binding>
          <mod-up>
            <binding>
              <command>property-assign</command>
              <property>controls/flight/elevator-trim-switch</property>
              <value>0</value>
            </binding>
          </mod-up>
        </action>

        <!-- Master -->
        <action>
          <name>Master</name>
          <button>0</button>
          <x>-330</x>
          <y>-62</y>
          <w>20</w>
          <h>20</h>
          <repeatable type="bool">false</repeatable>
          <binding>
            <command>property-toggle</command>
            <property>controls/electric/battery-switch</property>
          </binding>
        </action>

