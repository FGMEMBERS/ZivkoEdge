<?xml version = '1.0' encoding = 'UTF-8' ?>
<PropertyList>

  <path>MagnetoSwitch.ac</path>

  <panel>
    <path>Aircraft/ZivkoEdge/Models/MagnetoSwitchPanel.xml</path>
    <bottom-left>
      <x-m>0.0</x-m>
      <y-m>-0.02</y-m>
      <z-m>-0.02</z-m>
    </bottom-left>
    <bottom-right>
      <x-m>0.0</x-m>
      <y-m>0.02</y-m>
      <z-m>-0.02</z-m>
    </bottom-right>
    <top-left>
      <x-m>0.0</x-m>
      <y-m>-0.02</y-m>
      <z-m>0.02</z-m>
    </top-left>
  </panel>


  <animation>
    <type>noshadow</type>
  </animation>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>10</max-m>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Key</object-name>
    <object-name>KeyHole</object-name>
    <property>controls/engines/engine[0]/magneto-switch-position</property>
    <factor>30</factor>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

</PropertyList>
